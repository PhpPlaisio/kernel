{
  "name": "setbased/abc",
  "description": "ABC-Framework",
  "keywords": [
    "ABC-Framework"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "setbased/abc-babel": "^0.10.4",
    "setbased/abc-blob-store": "^1.1.0",
    "setbased/abc-canonical-hostname-resolver": "^1.0.0",
    "setbased/abc-company-resolver": "^1.0.0",
    "setbased/abc-error-logger": "^1.0.0",
    "setbased/abc-html": "^1.2.0",
    "setbased/abc-http-header": "^1.1.0",
    "setbased/abc-mail-message": "^0.9.0",
    "setbased/abc-language-resolver": "^1.0.0",
    "setbased/abc-lock-named": "^1.0.0",
    "setbased/abc-obfuscator": "^1.0.0",
    "setbased/abc-page": "^0.9.0",
    "setbased/abc-request": "^0.9.0",
    "setbased/abc-request-handler": "^1.0.0",
    "setbased/abc-request-logger": "^1.0.0",
    "setbased/abc-request-parameter-resolver": "^1.0.0",
    "setbased/abc-session": "^0.9.1",
    "setbased/exception": "^1.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^6.0.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
