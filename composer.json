{
  "name": "plaisio/kernel",
  "description": "PhpPlaisio: Kernel",
  "keywords": [
    "PhpPlaisio",
    "Kernel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.5.28"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
