{
  "name": "setbased/abc",
  "description": "SetBased ABC Framework",
  "type": "project",
  "keyword": [
    "Framework"
  ],
  "authors": [
    {
      "name": "Paul Water",
      "email": "p.r.water@setbased.nl",
      "homepage": "http://www.setbased.nl",
      "role": "Architect"
    }
