{
  "name": "setbased/abc",
  "description": "SetBased ABC Framework",
  "keywords": [
    "Framework"
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "2.*",
    "phpunit/phpunit": "4.*",
    "setbased/phing-extensions": "2.*"
  },
  "require": {
    "php": ">=5.4.0",
    "gajus/dindent": "^2.0.0",
    "ircmaxell/password-compat": "^1.0.3",
    "setbased/abc-blob-store": "^1.0.0",
    "setbased/exception": "^1.0.0",
    "setbased/abc-html": "^1.1.0",
    "setbased/abc-http-header": "^1.0.0",
    "setbased/abc-mail-message": "^0.9.0",
    "setbased/abc-obfuscator-interface": "^1.0.0",
    "setbased/abc-url": "^1.0.0",
    "setbased/php-stratum": "^0.9.45"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\Test\\": "test"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
