{
  "name": "plaisio/kernel",
  "description": "PhpPlaisio: Kernel",
  "keywords": [
    "PhpPlaisio",
    "Kernel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
