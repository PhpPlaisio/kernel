{
  "name": "setbased/abc",
  "description": "SetBased ABC Framework",
  "keywords": [
    "Framework"
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "*",
    "phpunit/phpunit": ">=4.0.0"
  },
  "require": {
    "php": ">=5.4.0",
    "gajus/dindent": "2.*",
    "ircmaxell/password-compat": ">=1.0.3",
    "setbased/abc-blob-store": "*",
    "setbased/exception": "1.*",
    "setbased/abc-html": "1.*",
    "setbased/abc-http-header": "*",
    "setbased/abc-obfuscator-interface": "1.*",
    "setbased/abc-url": "*",
    "setbased/php-affirm": "*",
    "setbased/php-stratum": "*"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
