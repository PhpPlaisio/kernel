{
  "name": "plaisio/kernel",
  "description": "PhpPlaisio: Kernel",
  "keywords": [
    "PhpPlaisio",
    "Kernel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
